<?xml version="1.0" encoding="UTF-8"?>
<portlet-app version="2.0"
   xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

<portlet>
    <portlet-name>jspPortlet</portlet-name>
    <display-name>jspPortlet</display-name>
    <portlet-class>javax.portlet.faces.GenericFacesPortlet</portlet-class>

    <init-param>
        <name>javax.portlet.faces.defaultViewId.view</name>
        <value>/portlets/sp_portlet.jsp</value>
    </init-param>
    <supports>
        <mime-type>text/html</mime-type>
        <portlet-mode>view</portlet-mode>
    </supports>
    <portlet-info>
        <title>JSP Portlet</title>
        <short-title>JSP Portlet</short-title>
    </portlet-info>
</portlet>
<portlet>
    <portlet-name>Sp_Portlet</portlet-name>
    <display-name>Sp_Portlet</display-name>
    <portlet-class>javax.portlet.faces.GenericFacesPortlet</portlet-class>

    <init-param>
        <name>javax.portlet.faces.defaultViewId.view</name>
        <value>/portlets/sp_portlet.jsp</value>
    </init-param>
    <supports>
        <mime-type>text/html</mime-type>
        <portlet-mode>view</portlet-mode>
    </supports>
    <portlet-info>
        <title>Sp Portlet</title>
        <short-title>Sp Portlet</short-title>
    </portlet-info>
</portlet>
</portlet-app>
