<?xml version="1.0" encoding="windows-1252"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" version="2.1">
  <application>
    <default-render-kit-id>org.apache.myfaces.trinidad.core</default-render-kit-id>
  </application>
  <managed-bean>
    <managed-bean-name>customerbean</managed-bean-name>
    <managed-bean-class>oracle.adffaces.handson.model.CustomerBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <navigation-rule>
    <navigation-case>
      <from-outcome>table_demo</from-outcome>
      <to-view-id>/Table.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>tree_demo</from-outcome>
      <to-view-id>/Tree.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>countrybean</managed-bean-name>
    <managed-bean-class>oracle.adffaces.handson.managed.bean.CountryBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>userbean</managed-bean-name>
    <managed-bean-class>oracle.adffaces.handson.managed.bean.UserBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>dialogbean</managed-bean-name>
    <managed-bean-class>oracle.adffaces.handson.managed.bean.DialogBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>singleselectionhandler</managed-bean-name>
    <managed-bean-class>oracle.adffaces.handson.model.SingleSelectionTableHandler</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>multiselectionhandler</managed-bean-name>
    <managed-bean-class>oracle.adffaces.handson.model.MultiSelectionTableHandler</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <navigation-rule>
    <navigation-case>
      <from-outcome>main_page</from-outcome>
      <to-view-id>/Main.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>treehandler</managed-bean-name>
    <managed-bean-class>oracle.adffaces.handson.model.TreeHandler</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>tablebean</managed-bean-name>
    <managed-bean-class>oracle.adffaces.handson.managed.bean.TableBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
</faces-config>