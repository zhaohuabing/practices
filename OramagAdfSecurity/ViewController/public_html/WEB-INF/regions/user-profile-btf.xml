<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="user-profile-btf">
    <default-activity>UserProfile</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>UserProfileBean</managed-bean-name>
      <managed-bean-class>adf.sample.managed.bean.UserProfileBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="UserProfile">
      <page>/userProfile/UserProfile.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
